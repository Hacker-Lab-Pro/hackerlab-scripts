    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeVersion": "18.20.4",
            "args": [
                "add",
                "bwapp"
            ],
            "env": {
                "HLB_DEBUG": "true"
            },
            "program": "${workspaceFolder}/src/index.js",
            "console": "integratedTerminal"
        }
    ]
}